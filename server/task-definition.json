{
  "family": "motivational-quotes-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::061051252873:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "motivational-quotes-container",
      "image": "061051252873.dkr.ecr.eu-west-1.amazonaws.com/motivational-quotes-server:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 3001,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/motivational-quotes-task",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
